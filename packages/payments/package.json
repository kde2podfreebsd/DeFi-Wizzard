{
  "name": "ruscamp-backend-template",
  "version": "1.0.0",
  "description": "",
  "author": "Tunas Digital",
  "private": true,
  "license": "LICENSED",
  "scripts": {
    "build": "rm -rf dist && nest build -p ./tsconfig.build.json",
    "build:start": "node dist/src/main.js",
    "build:start:dev": "dotenv -e dev.env -- node dist/src/main.js",
    "typecheck": "yarn tsc --noEmit",
    "start": "dotenv -e dev.env -- nest start --watch",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "env NODE_ENV=test jest --ci --reporters=default --reporters=jest-junit",
    "test:e2e": "dotenv -e e2e.env -- jest --config ./jest.e2e.js --runInBand --forceExit --detectOpenHandles --ci --reporters=default --reporters=jest-junit",
    "test:watch": "env NODE_ENV=test jest --watch",
    "test:cov": "jest --coverage",
    "pull": "git pull --recurse-submodules",
    "pull:submodules": "git submodule update --init --recursive --remote",
    "prisma:generate": "prisma generate",
    "migrate:dev": "dotenv -e dev.env -- prisma migrate dev",
    "migrate:e2e": "dotenv -e e2e.env -- prisma migrate deploy",
    "migrate:prod": "prisma migrate deploy && prisma db seed"
  },
  "dependencies": {
    "@camp/auth-client": "^3.1.3",
    "@nestjs/common": "^10.4.9",
    "@nestjs/core": "^10.4.9",
    "@nestjs/platform-express": "^10.4.9",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^5.22.0",
    "@transactional/core": "^1.2.0",
    "@transactional/prisma": "^1.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "convict": "^6.2.4",
    "express": "^4.21.1",
    "helmet": "^7.2.0",
    "morgan": "^1.10.0",
    "prisma": "^5.22.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.9",
    "@types/convict": "^6.1.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "7.9.0",
    "@typescript-eslint/parser": "7.9.0",
    "dotenv-cli": "^7.4.4",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "3.4.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed/seed.ts"
  }
}
